<%= form_with(model: @category, local: true) do |form| %>
  <p>
  <%= form.label :name %><br>
  <%= form.text_field :name %>
  </p>

  <p>
  <%= form.label :category_type %><br>
  <%= form.select :category_type, @types %>
  </p>

  <p>
  <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', categories_path %>
